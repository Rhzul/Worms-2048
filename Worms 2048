local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Worms 2048 by jngames V1", "Ocean")
 
--Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Skins Some probaly will not work only use one at a time")

MainSection:NewButton("Neon", "Skin", function()
-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "EquipSkin",
    [2] = "Neon"
}

game:GetService("ReplicatedStorage").Events.System.EquipRE:FireServer(unpack(args))
end)

